{
  "name": "anyui",
  "version": "0.0.6",
  "description": "gzip 5kb!  Any UI , such dom ... ,can adapt native App like ReactNative weex",
  "keywords": [
    "anyui"
  ],
  "main": "dist/index.js",
  "scripts": {
    "cleardist": "del-cli dist/**/*.js dist/**/*.js.map",
    "cleantest": "del-cli disttest/**/*.js disttest/**/*.js.map ",
    "clearall": "npm run cleardist & npm run cleantest",
    "build": "rollup -c",
    "build:bench": "rollup -c rollup.config.bench.js",
    "build:test": "rollup -w -c rollup.config.test.js",
    "build:debug": "tsc -w -t es5 -m commonjs",
    "build:all": "rollup -c & rollup -c rollup.config.bench.js & rollup -c rollup.config.test.js"
  },
  "dependencies": {
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "@types/node": "^8.0.24",
    "del-cli": "^1.1.0",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "throttle-debounce": "^1.0.1",
    "typescript": "^2.5.3"
  }
}